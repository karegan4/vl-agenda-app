<head>
<link rel="stylesheet"  type="text/css" href="/stylesheets/welcome.css">

</head>
<h1> Please log in. </h1>
<%= form_for @user, url: login_path, method: :post do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %><br><br>

    <%= f.label :email %>
    <%= f.email_field :email %> <br><br>   
  
    <%= f.label :password %>
    <%= f.password_field :password %> <br><br>
  
    <%= f.submit "Log In" %>
<% end %>